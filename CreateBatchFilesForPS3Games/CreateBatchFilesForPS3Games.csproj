<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <StartupObject>CreateBatchFilesForPS3Games.Program</StartupObject>
    <ApplicationIcon>icon\icon.ico</ApplicationIcon>
    <NoWin32Manifest>true</NoWin32Manifest>
    <PackageIcon>icon2.png</PackageIcon>
    <RepositoryType>git</RepositoryType>
    <UseWindowsForms>True</UseWindowsForms>
    <SupportedOSPlatformVersion>7.0</SupportedOSPlatformVersion>
    <AssemblyVersion>1.1</AssemblyVersion>
    <FileVersion>1.1</FileVersion>
    <RootNamespace>CreateBatchFilesForPS3Games</RootNamespace>
    <IsPackable>false</IsPackable>
  </PropertyGroup>

  <ItemGroup>
    <Resource Include="icon\icon.ico" />
    <None Remove="icon\icon.png" />
    <Resource Include="icon\icon.png" />
  </ItemGroup>

  <ItemGroup>
    <None Update="icon\icon2.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Compile Update="LogForm.cs">
      <SubType>Form</SubType>
    </Compile>
  </ItemGroup>

</Project>
